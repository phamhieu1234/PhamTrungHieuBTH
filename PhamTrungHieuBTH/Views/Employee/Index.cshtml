
@using PhamTrungHieuBTH.Models;
<h3> View Index</h3>
<h4>So sinh Nhan Vien: @ViewBag.SNV</h4>

<h2>Danh sách sinh viên</h2>
<hr>
<table class="table border">
    <tr>
        <th>
            <label>Id</label>
        </th>
        <th>
            <label>Name</label>
        </th>
        <th>
            <label>Tuổi</label>
        </th>
    </tr>
    @foreach (var item in ViewData["Employee"] as List<Employee>)  
    {
     <tr>
        <td>@item.EmployeeID</td>
        <td>@item.EmployeeName</td>
        <td>@item.Age</td>
     </tr>
    }
</table>